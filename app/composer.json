{
    "type": "project",
    "name": "erprof/erprof",
    "description": "",
    "license": "proprietary",
    "minimum-stability": "stable",
    "authors": [],
    "require": {
        "php": "^7.4.0",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-gmp": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-soap": "*",
        "action/id2-user": "^0.0.2",
        "action/logger": "^2.1",
        "aws/aws-sdk-php": "^3.143",
        "barryvdh/elfinder-flysystem-driver": "^0.2.1",
        "doctrine/doctrine-bundle": "^2.2.4",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/orm": "^2.6",
        "easycorp/easyadmin-bundle": "^2.1",
        "helios-ag/fm-elfinder-bundle": "^10.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-bundle": "^1.5",
        "mikehaertl/phpwkhtmltopdf": "^2.4",
        "monolog/monolog": "^2.2",
        "nelmio/api-doc-bundle": "^3.3",
        "nelmio/cors-bundle": "^2.1",
        "pluk77/symfony-sphinx-bundle": "^1.0@dev",
        "predis/predis": "^1.1",
        "qandidate/symfony-json-request-transformer": "^2.0",
        "ramsey/uuid": "^3.8",
        "roave/security-advisories": "dev-master",
        "sensio/framework-extra-bundle": "^5.2",
        "sentry/sentry-symfony": "^3.0",
        "snc/redis-bundle": "^3.2",
        "symfony/asset": "^5.2",
        "symfony/console": "^5.2.5",
        "symfony/dotenv": "^5.2",
        "symfony/flex": "^1.12.2",
        "symfony/form": "^5.2",
        "symfony/framework-bundle": "^5.2",
        "symfony/http-client": "^5.2",
        "symfony/lock": "^5.2",
        "symfony/maker-bundle": "^1.9",
        "symfony/monolog-bundle": "^3.3",
        "symfony/process": "^5.2",
        "symfony/serializer": "^5.2",
        "symfony/templating": "^5.2",
        "symfony/translation": "^5.2",
        "symfony/twig-bundle": "^5.2",
        "symfony/validator": "^5.2",
        "symfony/webpack-encore-bundle": "^1.8",
        "symfony/yaml": "^5.2",
        "vich/uploader-bundle": "^1.14"
    },
    "require-dev": {
        "action/codeception-testrail": "1.2",
        "codeception/codeception": "4.0",
        "codeception/module-asserts": "^1.1",
        "codeception/module-doctrine2": "^1.0",
        "codeception/module-rest": "^1.0",
        "codeception/module-symfony": "^1.0",
        "flow/jsonpath": "^0.4.0",
        "symfony/profiler-pack": "^1.0",
        "wiremock-php/wiremock-php": "^2.22"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "repositories": [
            {
                "type": "composer",
                "url": "https://asset-packagist.org"
            },
            {
                "packagist": true
            }
        ],
        "process-timeout": 1800,
        "sort-packages": true,
        "secure-http": false
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear --no-debug": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "elfinder:install": "symfony-cmd",
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        },
        "public-dir": "public/assets"
    },
    "repositories": [
        {"type": "composer", "url": "http://nexus.action-media.ru/repository/composer-hosted-action"}
    ]
}
